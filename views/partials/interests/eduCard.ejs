<div class="card text-center" id="<%edu.title%>">
    <div class="card-body">
        <h5 class="card-title">
            <%- edu.title %>
        </h5>
        <div class="row">
            <div class="">
                <button class="btn btn-info mr-1" data-toggle="modal" data-target="#diploma">
                    <%=edu["diploma"]["name"]%>
                </button>
                <div id="diploma" class="modal fade" tabindex="-1" role="dialog">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-body">
                                <img src="<%=edu.diploma.img%>" class="img-responsive" width="100%" height="100%">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="">
                <button class="btn btn-info mr-1" data-toggle="modal" data-target="#grades">Grades sheet</button>
                <div id="grades" class="modal fade" tabindex="-1" role="dialog" width="1000px">
                    <div class="modal-dialog modal-lg" width="1000px">
                        <div class="modal-content" width="1000px">
                            <div class="modal-body" width="1000px">
                                <embed src='<%=edu.grades%>' type="application/pdf" width="100%" height="600px">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="">
                <button class="btn btn-info mr-1" data-toggle="modal" data-target="#tlp">Final project</button>
                <div id="tlp" class="modal fade" tabindex="-1" role="dialog">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-body">
                                <embed src='<%=edu.final%>' type="application/pdf" width="100%" height="600px">
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>